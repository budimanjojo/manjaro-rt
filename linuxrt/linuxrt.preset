# mkinitcpio preset file for the 'linux-rt' package

ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-rt-manjaro"

PRESETS=('default' 'fallback')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-rt-manjaro.img"
#default_options=""

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/initramfs-rt-manjaro-fallback.img" 
fallback_options="-S autodetect"
